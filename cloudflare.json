{
    "name": "ServiceUptime",
    "description": "ServiceUptime Website Monitoring - is remote website uptime monitoring designed to help you detect website downtime which can mean lost revenue and profits. Let ServiceUptime monitor your website 24/7/365 and alert you immediately upon your website accessibility, availability or performance problems.",
    "category": "monitoring",
    "version": "0.1",
	"main": "public/javascripts/serviceuptime.js",
    "account": {
        "callback_url": "https://www.serviceuptime.com/cf/",
        "user_fields": [
            "email"
        ]
    },
    "billing": {
        "type": "zone",
        "plans": [
            {
                "name": "Standard",
                "price": "4.95"
            },
            {
                "name": "Advanced",
                "price": "9.95"
            }
            ,{
                "name": "Professional",
                "price": "20.00"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/serviceuptime/cloudflare.git"
    },
    "config": {
        "assets": {
            "logos": {
                "200px": "./public/images/logo200px.png",
                "132px": "./public/images/logo132px.png"
            },
            "detail_page": [
                "./public/images/logo.png"
            ]
        },
        "details": {
            "Category": "monitoring",
            "Language": "English",
            "Price": "Free, but can be upgraded to paid options.",
            "Restrictions": "Free accounts can only monitor one website.",
            "SiteUptime": "<a href='http://www.serviceuptime.com/terms.php' target='_blank'>Terms of Service</a>",
			"has_js": true
        },
		"interface": [
			{
				"type": "hidden",
				"id": "engine_key",
				"name": "[siteuptime engine key]",
				"value": "missing",
				"domain_request": true
			}
		]
    }
}
